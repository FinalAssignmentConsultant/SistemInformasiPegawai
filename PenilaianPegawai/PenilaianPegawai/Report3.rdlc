      <Query>
        <DataSourceName>PenilaianPegawaiConnectionString</DataSourceName>
        <CommandText>SELECT        Pegawai.NIK, [Unit Kerja].[nama unit kerja], [Komponen Kapabil].[nama komponen], Kapabilitas.nilai, Kapabilitas.bulan, Kapabilitas.tahun, 
                         Pegawai.Nama
FROM            (((Pegawai INNER JOIN
                         [Unit Kerja] ON Pegawai.idunitkerja = [Unit Kerja].idunitkerja) INNER JOIN
                         Kapabilitas ON Pegawai.NIK = Kapabilitas.NIK) INNER JOIN
                         [Komponen Kapabil] ON Kapabilitas.idkomponenkapabilitas = [Komponen Kapabil].idkomponenkapabilitas)
WHERE        (Kapabilitas.bulan = ?) AND (Kapabilitas.tahun = ?) AND ([Unit Kerja].[nama unit kerja] = ?)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
